{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "single"],
        "endOfLine": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": "warn",
        "class-methods-use-this": "off"
    }
}
